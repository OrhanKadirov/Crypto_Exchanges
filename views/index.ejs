<!DOCTYPE html>
<html>

<head>
  <title>
    <%= pageTitle %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <nav>
    <ul>
      <% exchanges.map((exchange)=> { %>
        <li>
          <a href=" <%= exchange.website %> ">
            <%= exchange.name %>
          </a>
        </li>
        <% }) %>
    </ul>
  </nav>

  <div>
    <h1>
      <%= pageTitle %>
    </h1>
    <h3>Welcome to <%= pageTitle %>
    </h3>
  </div>

  <ul class="ul">
    <% exchanges.map((exchange)=> { %>
      <li>
        <a href=" <%= exchange.website %> ">

          <%= exchange.name %>
        </a>
        <%= "Volume 24h: " %>
          <%= exchange.volume_24h.toFixed(2) %>
      </li>
      <% }) %>
  </ul>

  <p class="popular_100_coins">
    <a href="./coins" target="_blank" rel="noopener noreferrer">
      <%= "100 Most Popular Coins" %>
    </a>
  </p>

  <footer>
    <p>Copyright Â© 2022 Orhan Kadirov All Right Reserved</p>
  </footer>
</body>

</html>